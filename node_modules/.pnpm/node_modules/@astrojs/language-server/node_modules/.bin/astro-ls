#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules/@astrojs/language-server/bin/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules/@astrojs/language-server/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules/@astrojs/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules/@astrojs/language-server/bin/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules/@astrojs/language-server/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules/@astrojs/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/@astrojs+language-server@2._8712300a40a9691fb11e339e526a1779/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../../bin/nodeServer.js" "$@"
fi
