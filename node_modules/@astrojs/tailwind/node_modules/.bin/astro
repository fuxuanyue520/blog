#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/astro@4.16.19_rollup@4.53.5_typescript@5.9.3/node_modules/astro/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/astro@4.16.19_rollup@4.53.5_typescript@5.9.3/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/astro@4.16.19_rollup@4.53.5_typescript@5.9.3/node_modules/astro/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/astro@4.16.19_rollup@4.53.5_typescript@5.9.3/node_modules:/mnt/c/Users/24172/Desktop/X_blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@4.16.19_rollup@4.53.5_typescript@5.9.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@4.16.19_rollup@4.53.5_typescript@5.9.3/node_modules/astro/astro.js" "$@"
fi
